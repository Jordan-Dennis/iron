set terminal pdf 
set output "pub/q1a.pdf"
set key noautotitle
set palette defined (0 "black", 1 "white")
unset xtics
unset ytics
unset y2tics
unset border 
unset colorbox
set multiplot layout 5, 1 title "Spin Evolution"
    do for [i=1:5] {
        set ylabel "T=".i
        set size ratio 0.02
        plot "pub/data/1a.csv" using 2:1:(i + 2) with image 
    }
unset multiplot

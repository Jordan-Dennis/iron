#set terminal wxt persist 
set output "pub/figures/q2a.pdf"
set key noautotitle
set palette defined (0 "black", 1 "white")
unset xtics
unset ytics
unset y2tics
unset border 
unset colorbox
set multiplot layout 1,3 title "Spin Evolution"
    do for [i=1:3] {
        set ylabel "T=".i
        set size ratio 2
        plot "pub/data/2a.csv" using 2:1:(column(i + 2)) with image 
    }
unset multiplot

set terminal gif animate delay 0.05
set size ratio 0.02
set output "pub/ising.gif"
set xrange [0:100]
set yrange [-.5:1.5]
set key noautotitle
unset xtics
unset ytics
unset title
unset y2tics
unset border 
unset colorbox 
do for [i=1:1000] {
    plot "pub/data/ising.csv" using 2:1:(column(i + 2)) with image
}

